(=>
  (instance ?C ChurningButter)
  (hasPurpose ?C
    (exists (?M ?R ?B)
      (and
        (instance ?M Milk)
        (instance ?R Rotating)
        (patient ?R ?M)
        (result ?R ?B)
        (instance ?B Butter)))))
        
(=>
  (and
    (instance ?C ChurningButter)
    (instance ?E Emulsion)
    (patient ?C ?E))
  (exists (?M ?B)
    (and
      (holdsDuring
        (BeginFn
          (WhenFn ?C))
        (and
          (equal ?E ?M)
          (instance ?M Milk)))
      (holdsDuring
        (EndFn
          (WhenFn ?C))
        (and
          (equal ?E ?B)
          (instance ?B Butter))))))
     
